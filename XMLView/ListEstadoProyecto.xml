<?xml version="1.0" encoding="UTF-8"?>
<view>
    <columns>
        <column name="code" order="100">
            <widget type="text" fieldname="idestado" />
        </column>
        <column name="name" order="110">
            <widget type="text" fieldname="nombre" />
        </column>
        <column name="editable" display="center" order="120">
            <widget type="checkbox" fieldname="editable" />
        </column>
        <column name="default" display="center" order="130">
            <widget type="checkbox" fieldname="predeterminado" />
        </column>
    </columns>
    <rows>
        <row type="status">
            <option color="info" fieldname="predeterminado">0</option>
            <option color="warning" fieldname="editable">0</option>
            <option color="success" fieldname="editable">1</option>
        </row>
    </rows>
</view>
